@model TestsProject.Models.Entities.TestStats
@{
	Layout = "~/Views/Shared/_Layout.cshtml";
	ViewBag.Title = "Past Test";
}
<h3>@Model.TestParameters.Name - @Html.DisplayFor(s => Model.StartTime)</h3>
<h4>@Html.ActionLink("Details", "Full","Test", new {id=Model.Id},new {})</h4>
<div class="row">
	<div class="col-md-3">
		@Html.LabelFor(m => Model.Correct):
		@Html.ValueFor(m => Model.Correct)
		<br />
		@Html.LabelFor(m => Model.Incorrect):
		@Html.ValueFor(m => Model.Incorrect)
		<br />
		@Html.LabelFor(m => Model.Unanswered):
		@Html.ValueFor(m => Model.Unanswered)
		<br />
		@Html.LabelFor(m => Model.PartiallyCorrect):
		@Html.ValueFor(m => Model.PartiallyCorrect)
		<br />
		@Html.LabelFor(m => Model.PartiallyIncorrect):
		@Html.ValueFor(m => Model.PartiallyIncorrect)
		<br/>
		<hr/>
		@Html.LabelFor(m => Model.TotalScore):
		@Html.ValueFor(m => Model.TotalScore)
		<br />

		@Html.LabelFor(m => Model.MaximumScore):
		@Html.ValueFor(m => Model.MaximumScore)
	</div>
	<div class="col-md-6">
		<br />
		<img src="@Url.Action("ChartTestResults","Chart", new {id = @Model.Id})" />
		<br />
	</div>
</div>
@Html.ActionLink("<<Back", "Index", "Student")